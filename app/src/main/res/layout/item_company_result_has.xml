<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/bind_company_result_has"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <View
        android:id="@+id/bind_company_result_tip_view"
        android:layout_width="@dimen/dp_4"
        android:layout_height="@dimen/dp_28"
        android:layout_marginLeft="@dimen/dp_48"
        android:layout_marginTop="@dimen/dp_50"
        android:background="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/bind_company_result_tip_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_16"
        android:text="请确认具体绑定信息"
        android:textColor="@color/white"
        android:textSize="@dimen/sp_28"
        app:layout_constraintBottom_toBottomOf="@+id/bind_company_result_tip_view"
        app:layout_constraintStart_toEndOf="@+id/bind_company_result_tip_view"
        app:layout_constraintTop_toTopOf="@+id/bind_company_result_tip_view" />

    <View
        android:id="@+id/bind_company_result_view"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_461"
        android:layout_marginLeft="@dimen/dp_24"
        android:layout_marginTop="@dimen/dp_24"
        android:layout_marginRight="@dimen/dp_24"
        android:background="@drawable/bg_round_corner"
        app:layout_constraintTop_toBottomOf="@+id/bind_company_result_tip_view" />

    <TextView
        android:id="@+id/bind_company_result_item_top_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/dp_24"
        android:text="绑定信息"
        android:textColor="@color/color_333333"
        android:textSize="@dimen/sp_28"
        app:layout_constraintStart_toStartOf="@+id/bind_company_result_view"
        app:layout_constraintTop_toTopOf="@+id/bind_company_result_view" />

    <TextView
        android:id="@+id/bind_company_result_item_text1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_24"
        android:layout_marginTop="@dimen/dp_50"
        android:layout_marginRight="@dimen/dp_24"
        android:text="手机号码"
        android:textColor="@color/color_666666"
        android:textSize="@dimen/sp_28"
        app:layout_constraintStart_toStartOf="@+id/bind_company_result_view"
        app:layout_constraintTop_toBottomOf="@+id/bind_company_result_item_top_text" />

    <TextView
        android:id="@+id/bind_company_result_item_text1r"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="@dimen/dp_24"
        android:text="手机号码"
        android:textColor="@color/color_666666"
        android:textSize="@dimen/sp_28"
        app:layout_constraintBottom_toBottomOf="@+id/bind_company_result_item_text1"
        app:layout_constraintEnd_toEndOf="@+id/bind_company_result_view"
        app:layout_constraintTop_toTopOf="@+id/bind_company_result_item_text1" />

    <TextView
        android:id="@+id/bind_company_result_item_text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/dp_24"
        android:text="所在城市"
        android:textColor="@color/color_666666"
        android:textSize="@dimen/sp_28"
        app:layout_constraintStart_toStartOf="@+id/bind_company_result_view"
        app:layout_constraintTop_toBottomOf="@+id/bind_company_result_item_text1" />

    <TextView
        android:id="@+id/bind_company_result_item_text2r"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="@dimen/dp_24"
        android:text="手机号码"
        android:textColor="@color/color_666666"
        android:textSize="@dimen/sp_28"
        app:layout_constraintBottom_toBottomOf="@+id/bind_company_result_item_text2"
        app:layout_constraintEnd_toEndOf="@+id/bind_company_result_view"
        app:layout_constraintTop_toTopOf="@+id/bind_company_result_item_text2" />


    <TextView
        android:id="@+id/bind_company_result_item_text3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/dp_24"
        android:text="客户名称"
        android:textColor="@color/color_666666"
        android:textSize="@dimen/sp_28"
        app:layout_constraintStart_toStartOf="@+id/bind_company_result_view"
        app:layout_constraintTop_toBottomOf="@+id/bind_company_result_item_text2" />

    <TextView
        android:id="@+id/bind_company_result_item_text3r"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="@dimen/dp_24"
        android:text="手机号码"
        android:textColor="@color/color_666666"
        android:textSize="@dimen/sp_28"
        app:layout_constraintBottom_toBottomOf="@+id/bind_company_result_item_text3"
        app:layout_constraintEnd_toEndOf="@+id/bind_company_result_view"
        app:layout_constraintTop_toTopOf="@+id/bind_company_result_item_text3" />

    <TextView
        android:id="@+id/bind_company_result_item_text4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/dp_24"
        android:text="客户地址"
        android:textColor="@color/color_666666"
        android:textSize="@dimen/sp_28"
        app:layout_constraintStart_toStartOf="@+id/bind_company_result_view"
        app:layout_constraintTop_toBottomOf="@+id/bind_company_result_item_text3" />

    <TextView
        android:id="@+id/bind_company_result_item_text4r"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="@dimen/dp_24"
        android:ellipsize="end"
        android:lines="1"
        android:maxEms="15"
        android:text="手机号码"
        android:textColor="@color/color_666666"
        android:textSize="@dimen/sp_28"
        app:layout_constraintBottom_toBottomOf="@+id/bind_company_result_item_text4"
        app:layout_constraintEnd_toEndOf="@+id/bind_company_result_view"
        app:layout_constraintTop_toTopOf="@+id/bind_company_result_item_text4" />

    <TextView
        android:id="@+id/bind_company_result_item_text5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/dp_24"
        android:text="客户税号"
        android:textColor="@color/color_666666"
        android:textSize="@dimen/sp_28"
        app:layout_constraintStart_toStartOf="@+id/bind_company_result_view"
        app:layout_constraintTop_toBottomOf="@+id/bind_company_result_item_text4" />

    <TextView
        android:id="@+id/bind_company_result_item_text5r"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="@dimen/dp_24"
        android:text="手机号码"
        android:textColor="@color/color_666666"
        android:textSize="@dimen/sp_28"
        app:layout_constraintBottom_toBottomOf="@+id/bind_company_result_item_text5"
        app:layout_constraintEnd_toEndOf="@+id/bind_company_result_view"
        app:layout_constraintTop_toTopOf="@+id/bind_company_result_item_text5" />

</androidx.constraintlayout.widget.ConstraintLayout>