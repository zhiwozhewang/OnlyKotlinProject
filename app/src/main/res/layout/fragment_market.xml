<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    <ImageView
        android:id="@+id/market_back"
        android:layout_width="@dimen/dp_44"
        android:layout_height="@dimen/dp_44"
        android:layout_margin="@dimen/dp_24"
        android:layout_marginStart="@dimen/dp_24"
        android:src="@mipmap/im_market_search"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/market_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/dp_24"
        android:gravity="center"
        android:text="集市"
        android:textColor="@color/black"
        android:textSize="@dimen/sp_36"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/market_message"
        android:layout_width="@dimen/dp_44"
        android:layout_height="@dimen/dp_44"
        android:layout_margin="@dimen/dp_24"
        android:layout_marginStart="@dimen/dp_24"
        android:src="@mipmap/im_market_message"
        app:layout_constraintEnd_toStartOf="@+id/market_server"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/market_server"
        android:layout_width="@dimen/dp_44"
        android:layout_height="@dimen/dp_44"
        android:layout_margin="@dimen/dp_24"
        android:layout_marginStart="@dimen/dp_24"
        android:src="@mipmap/im_market_server"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.enn.ionic.ui.view.WebViewWrapper
        android:id="@+id/web_market"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_0"
        android:layout_marginTop="@dimen/dp_24"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/market_title" />

</androidx.constraintlayout.widget.ConstraintLayout>